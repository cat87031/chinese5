        // 顯示生字詳情
        function showCharacterDetail(character, bopomofo, radical, strokes, structure, element) {
            // 移除所有項目的active類
            document.querySelectorAll('.vocabulary-item').forEach(item => {
                item.classList.remove('active');
            });
            
            // 為當前點擊的項目添加active類
            if (element) {
                element.classList.add('active');
            }
            
            const detailSection = document.getElementById('character-detail');
            const data = characterData[character] || {
                words: ['詞1', '詞2', '詞3', '詞4', '詞5'],
                idioms: ['成語1', '成語2', '成語3'],
                sentences: ['例句1', '例句2'],
                meaning: ['字義1', '字義2', '字義3'],
                eduUrl: 'https://pedia.cloud.edu.tw/',
                idiomUrl: 'https://dict.idioms.moe.edu.tw/'
            };
            
            document.getElementById('char-large').textContent = character;
            document.getElementById('char-bopomofo').textContent = bopomofo;
            document.getElementById('char-radical').textContent = radical;
            document.getElementById('char-strokes').textContent = strokes;
            document.getElementById('char-structure').textContent = structure;
            
            // 更新詞語列表
            const wordsList = document.getElementById('char-words');
            wordsList.innerHTML = '';
            data.words.forEach(word => {
                const li = document.createElement('li');
                li.textContent = word;
                wordsList.appendChild(li);
            });
            
            // 更新成語列表
            const idiomsList = document.getElementById('char-idioms');
            idiomsList.innerHTML = '';
            data.idioms.forEach(idiom => {
                const li = document.createElement('li');
                li.textContent = idiom;
                idiomsList.appendChild(li);
            });
            
            // 更新例句列表
            const sentencesList = document.getElementById('char-sentences');
            sentencesList.innerHTML = '';
            data.sentences.forEach(sentence => {
                const li = document.createElement('li');
                li.innerHTML = sentence;
                sentencesList.appendChild(li);
            });
            
            // 更新字義列表
            const meaningList = document.getElementById('char-meaning');
            meaningList.innerHTML = '';
            data.meaning.forEach(meaning => {
                const li = document.createElement('li');
                li.textContent = meaning;
                meaningList.appendChild(li);
            });
            
            // 更新外部連結
            document.getElementById('edu-link').href = data.eduUrl;
            document.getElementById('edu-link').innerHTML = '<i class="fas fa-external-link-alt"></i> 教育百科 - ' + character;
            
            document.getElementById('idiom-link').href = data.idiomUrl;
            document.getElementById('idiom-link').innerHTML = '<i class="fas fa-external-link-alt"></i> 成語典 - ' + character + '相關成語';
            
            // 顯示詳情區塊
            detailSection.style.display = 'block';
            
            // 滾動到詳情區塊
            detailSection.scrollIntoView({ behavior: 'smooth' });
        }
        
        // 滾動到指定區塊
        function scrollToSection(sectionId) {
            const section = document.getElementById(sectionId);
            if (section) {
                section.scrollIntoView({ behavior: 'smooth' });
            }
        }
        
        // 初始顯示第一個生字的詳情
        document.addEventListener('DOMContentLoaded', function() {
            showCharacterDetail('蟲', 'ㄔㄨㄥˊ', '虫', 18, '左右結構');
        });
    </script>
</body>
</html>
